
<div class="formBox">
  <h1>Crear un nuevo proyecto</h1>
  
  <%= error_messages_for :project %>
  
  <% form_for(@project) do |f| %>
    <div><label>
      Nombre</label>
      <%= f.text_field :name %>
    </div>
  
  <%= submit_button( 'Crear', "jQuery('#new_project').submit()" ) %>

  <% end %>
</div>


