<h1>c a l c pages - Proyectos</h1>

<%  action_area do %>
    <%= action 'report_add.png', 'New project', new_admin_project_path %>
  <% end %>


<table>
  <thead>
    <tr>
      <td>Id</td>
      <td>Nombre</td>
      <td></td>
    </tr>
  </thead>
  
  <tbody>
    <% for project in @projects %>
      <tr>
        <td><%= project.id  %>
        <td><%= link_to project.name, admin_project_pages_path(project) %></td>
        <td class="table_actions">
                 <%=  link_to 'Edit', edit_admin_project_path(project) %>
                <%= link_to 'Destroy', admin_project_path(project), :confirm => 'Are you sure?', :method => :delete %>
        </td>
      </tr>
    <% end %>
  </tbody>
  
</table>

<br />


